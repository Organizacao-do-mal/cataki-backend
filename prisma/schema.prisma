generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Collector {
  id String @id @default(uuid())

  @@map("collectors")
}

model Supporter {
  id        String  @id @default(uuid())
  name      String
  cpf       String  @unique
  latitude  Decimal
  longitude Decimal

  @@map("supporters")
}

model Organization {
  id        String  @id @default(uuid())
  name      String
  cnpj      String  @unique
  latitude  Decimal
  longitude Decimal

  @@map("organizations")
}
